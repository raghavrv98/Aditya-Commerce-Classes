<section id="home" class="padding_top" style="color:black">
    <div class="container">
        <h2 style="text-align:center"> Create Exam</h2><br>
        <div class="col-xs-12 col-sm-12 col-lg-12">
        <form method="post" action = "/admin-create-exam">
        <table class="table table-hover table-responsive">
            <thead>
              <tr>
                <th scope="col">Serial no.</th>
                <th scope="col">Date</th>
                <th scope="col">Batch Time</th>
                <th scope="col">Test Time</th>
                <th scope="col">Subject</th>
                <th scope="col">Maximum Marks</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <th scope="row">1</th>
                <td><input type="date" name="testDate"></td>
                <td><input type="time" name="batchtime"></td>
                <td><input type="time" name="testtime"></td>
                <td>
                  <select name="courseId" class="custom-select" style="width:250px;">
                    <option selected>Subjects</option>
                    <option value="1">Accounts</option>
                    <option value="3">Business Studies</option>
                    <option value="2">Economics</option>
                    <option value="4">English</option>
                  </select></td>
                  <td><input type="number" name="maxMarks"></td>
            </tr>
            </tbody>
          </table>
          <center><input type="submit" class="button btnprimary top50" style="background-color:#00bcd4" name="" value="Submit"></center>
                    
        </form>
        </div>
    </div>
